    CONTROL "", IDC_FIND_EDIT, EDIT, 0x40000000 | 0x50000000 | 0x0001, 10, 10, 180, 20
    CONTROL "", IDC_REPLACE_EDIT, EDIT, 0x40000000 | 0x50000000 | 0x0001, 10, 40, 180, 20
    // Add other controls as needed
}